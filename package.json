{
  "name": "etrie_c",
  "version": "1.0.0",
  "description": "Equestrian center web site",
  "main": "index.php",
  "scripts": {
    "build": "npm run lessmain && npm run lesscarousel && npm run lessaccommodation && npm run lessactivities && npm run lesssections && npm run lesspresentation && npm run lessservice && npm run lesscontact && npm run lesswelcome && npm run lessgallery && npm run lessplace && npm run lesspartenar && npm run lesslegal && npm run phpindex && npm run phpaccommodation && npm run phpgallery && npm run phppartenar && npm run phpplace && npm run phpstables1 && npm run phpstables2 && npm run phpcontact && npm run phplegal && npm run copyFiles",
    "lessmain": "lessc ./src/style/main.less build/style/main.css",
    "lesscarousel": "lessc ./src/style/carousel.less build/style/carousel.css",
    "lessaccommodation": "lessc ./src/style/accommodation/accommodation.less build/style/accommodation/accommodation.css",
    "lessactivities": "lessc ./src/style/accommodation/activities.less build/style/accommodation/activities.css",
    "lesssections": "lessc ./src/style/accommodation/sections.less build/style/accommodation/sections.css",
    "lesspresentation": "lessc ./src/style/stables/presentation.less build/style/stables/presentation.css",
    "lessservice": "lessc ./src/style/stables/service.less build/style/stables/service.css",
    "lesscontact": "lessc ./src/style/contact/contact.less build/style/contact/contact.css",
    "lesswelcome": "lessc ./src/style/welcomePage/welcome.less build/style/welcomePage/welcome.css", 
    "lessgallery": "lessc ./src/style/gallery.less build/style/gallery.css",
    "lessplace": "lessc ./src/style/place/place.less build/style/place/place.css",
    "lessslider": "lessc ./src/style/slider.less build/style/slider.css",
    "lesspartenar": "lessc ./src/style/partenar/partenar.less build/style/partenar/partenar.css",
    "lesslegal": "lessc ./src/style/legal.less build/style/legal.css",
    "phpindex": "php src/index.php -> build/index.html",
    "phpaccommodation": "php src/accommodation.php -> build/accommodation.html",
    "phpgallery": "php src/gallery.php -> build/gallery.html",
    "phppartenar": "php src/partenar.php -> build/partenar.html",
    "phpplace": "php src/place.php -> build/place.html",
    "phpstables1": "php src/stables1.php -> build/stables1.html",
    "phpstables2": "php src/stables2.php -> build/stables2.html",
    "phpcontact": "php src/contact.php -> build/contact.html",
    "phplegal": "php src/legal.php -> build/legal.html",
    "copyFiles": "cp -rf src/plugins build/plugins && cp -rf src/media build/media && cp -rf src/fonts build/fonts && mkdir build/templates && cp -rf src/templates/menuIcons build/templates/menuIcons && cp -rf src/templates/sliderIcons build/templates/sliderIcons && cp -f public/* build/"
  },
  "devDependencies": {
    "less": "^3.11.1",
    "php": "^1.0.2"
  },
  "author": "William Thiery",
  "license": "MIT"
}
